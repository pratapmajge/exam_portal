# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:5001

# Authentication
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id

# WebSocket Configuration
NEXT_PUBLIC_WS_URL=ws://localhost:5001

# Feature Flags
VITE_ENABLE_PROCTORING=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_NOTIFICATIONS=true

# Proctoring Settings
VITE_FACE_DETECTION_INTERVAL=1000
VITE_AUDIO_MONITORING_INTERVAL=1000
VITE_SCREENSHOT_INTERVAL=30000
VITE_VIOLATION_WARNING_THRESHOLD=2
VITE_VIOLATION_TERMINATION_THRESHOLD=3

# UI Settings
VITE_THEME=light
VITE_LANGUAGE=en
VITE_TIMEZONE=UTC

# Analytics
VITE_ENABLE_ANALYTICS=false
VITE_ANALYTICS_ID=

# Security
VITE_ENABLE_SSL=false
VITE_ENABLE_2FA=false 